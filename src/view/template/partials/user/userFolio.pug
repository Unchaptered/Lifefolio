div#access__folioCover
    if folioDB.shortcutView===true
        div.folio__contentBox
            // 표지 95% 95% (마진 2.5% 상하좌우)
            a.coverData(href=`/folio/${folioDB._id}`)
                img(src=`/${folioDB.shortcutUrl}`)
    else
        div.folio__contentBoxGrid
            // 표지 20% 2
            div.headerData
                a(href=`/folio/${folioDB._id}`)
                    header=folioDB.folioName
            // 메타 30% 3
            div.metaData
                span=folioDB.folioCreated
                if folioDB.folioDescription
                    span=folioDB.folioDescription
                else    
                    span
                a(href=`/user/profile/${String(folioDB.master)}`) 
                    span 아티스트 #{folioDB.masterName}
            // 프리뷰 100% 10
            div.preview
                if loggedIn
                    if String(folioDB.master)===loggedInUser._id
                        if folioDB.folioImageNameArray[0]!==""
                            div.preview-border
                                a(href=`/folio/${folioDB._id}`)
                                    svg(xmlns="http://www.w3.org/2000/svg" class="h-5 w-5" viewBox="0 0 20 20" fill="currentColor")
